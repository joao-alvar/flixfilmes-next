  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": "1.22.19"
